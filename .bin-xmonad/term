#!/bin/bash
#
# Launch terminals
#
# Author: Thomas Fr√∂ssman  ( thomasf@jossystem.se / http://thomas.jossystem.se )
#
#

set -e

cmd=${1}
# FIXME this just ignores the l/d/o options.. this script is deprecated so best to remove all references to it.
case $cmd in
    l|light)
        shift 1
        ;;
    d|dark)
        shift 1
        ;;
    o|other)
        shift 1
        ;;
esac

exec urxvt "$@"
